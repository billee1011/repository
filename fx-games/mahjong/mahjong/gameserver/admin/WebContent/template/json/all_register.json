{
        chart: {
        	type:'column',
        	width:800,
        	height:600,
        	options3d: {
                enabled: true,
            }
        },
        plotOptions:{
        	column:{
        		dataLabels:{
        			enabled:true
        		}
        	}
        },
        title: {
            text: '累计注册'
        },
        xAxis:{
        	categories: eval('('+'${data1}'+')')
        },
        credits: {
            enabled: false
        },
        series: [{
        	name:"累计注册数",
        	data:eval('('+'${data2}'+')')
        }]
 }